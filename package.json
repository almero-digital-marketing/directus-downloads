{
	"name": "directus-downloads",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.2",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "15.0.0"
	},
	"dependencies": {
		"glob": "^11.0.3",
		"mime-types": "^3.0.1"
	}
}
